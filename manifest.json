{
  "name": "Fast Tools",
  "version": "1.3",
  "description": "Integration of features that most developers and artists use",
  "permissions": [
    "contextMenus",
    "*://*/*"
  ],
  "background": {
    "scripts": [
      "utils.js",
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "display tooltip",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}